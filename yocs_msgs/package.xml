  <description>Yujin's Open Control System messages, services and actions</description>

  <author email="jorge@rnd.yujinrobot.com">Jorge Santos</author>
  <maintainer email="jorge@rnd.yujinrobot.com">Jorge Santos</maintainer>
  <license>BSD</license>
  <url type="website">http://ros.org/wiki/yocs_msgs</url>
  <url type="repository">https://github.com/yujinrobot/yujin_ocs</url>
  <url type="bugtracker">https://github.com/yujinrobot/yujin_ocs/issues</url>
  
